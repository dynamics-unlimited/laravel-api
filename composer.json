{
  "name": "dynamics-unlimited/laravel-api",
  "description": "Support package for Laravel based API projects",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Kairnial\\LaravelApi\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "ext-openssl": "*",
    "firebase/php-jwt": "^6.3",
    "guzzlehttp/guzzle": "^7.2",
    "laravel/sanctum": "^3.2",
    "laravel/framework": "^10",
    "spatie/laravel-translation-loader": "^2.7.6",
    "juststeveking/laravel-redoc": "^2.0",
    "darkaonline/l5-swagger": "^8.5",
    "dynamics-unlimited/laravel-common-package": "^1.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.21",
    "mockery/mockery": "^1.5",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.0"
  },
  "authors": [
    {
      "name": "Nicolas GIROD",
      "email": "nicolas.girod@thinkproject.com"
    }
  ],
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Kairnial\\LaravelApi\\Providers\\LaravelApiServiceProvider",
        "Kairnial\\LaravelApi\\Providers\\AuthServiceProvider"
      ]
    }
  }
}
